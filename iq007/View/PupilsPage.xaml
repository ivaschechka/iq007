<Page x:Class="iq007.View.PupilsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:iq007.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PupilsPage" ShowsNavigationUI="False">
    <Page.Effect>
        <DropShadowEffect Color="#FF686868" ShadowDepth="1"/>
    </Page.Effect>

    <Grid Background="White" Margin="0,0,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="257"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="frame" 
               Content="Frame" 
               Grid.Column="0"
               BorderBrush="#FF323232" 
               Source="/View/AddPupilPage.xaml"/>
        <Frame x:Name="frame1" 
               Content="Frame" 
               Grid.Column="1" 
               Source="/View/ListPupilNPage.xaml" Margin="30"/>
    </Grid>
</Page>
